/* pages/camera/camera.wxss */.camera {
  width: 100%;
  height: 100vh;
  /* border-radius: 50%; */
  margin: 20px auto 0;
  position: relative;
}

.camera image {
  position: absolute;
  top: 20%;
  width: 390px;
  height: 390px;
  z-index: 10;
}

.camera camera {
  width: 100%;
  height: 100%;
}

button.takePhoto:not([size='mini']) {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100vw;
  height: 90rpx;
  border-radius: 0;
}

